009010), 2015."],"concepts":[{"name":"Learning Disability - Primary Care","concept_id":"C2051","concept_version_id":5240,"coding_system":"OXMIS codes","codes":[{"code":"3061LF","description":"LEARNING DIFFICULTY","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"OXMIS","Category":"diagnostic"},{"code":"311","description":"MENTAL RETARDATION MILD","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"OXMIS","Category":"diagnostic"},{"code":"312","description":"MENTAL RETARDATION MODERATE","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"OXMIS","Category":"diagnostic"},{"code":"313","description":"MENTAL RETARDATION SEVERE","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"OXMIS","Category":"diagnostic"},{"code":"315","description":"RETARDATION MENTAL","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"OXMIS","Category":"diagnostic"},{"code":"315 HC","description":"MENTAL HANDICAP","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"OXMIS","Category":"diagnostic"}]},{"name":"Learning Disability - Primary Care","concept_id":"C2050","concept_version_id":5238,"coding_system":"Read codes v2","codes":[{"code":"13Z4E00","description":"Learning difficulties","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"Read","Category":"diagnostic"},{"code":"E2F2.00","description":"Other specific learning difficulty","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"Read","Category":"diagnostic"},{"code":"E3...00","description":"Mental retardation","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"Read","Category":"diagnostic"},{"code":"E30..00","description":"Mild mental retardation, IQ in range 50-70","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"Read","Category":"diagnostic"},{"code":"E31..00","description":"Other specified mental retardation","Entity type":"res26: LearningDis","Coding System (OXMIS Read)":"Read","Category":"diagnostic"},